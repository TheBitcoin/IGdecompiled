<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/blue_primary"/>
            <corners android:topRightRadius="@dimen/border_radius_m" android:bottomRightRadius="@dimen/border_radius_m"/>
        </shape>
    </item>
    <item android:gravity="left" android:width="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>
