<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="false">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <solid android:color="@color/switch_thumb_unchecked"/>
            <corners android:radius="24dp"/>
            <size android:height="24dp" android:width="48dp"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <solid android:color="@color/switch_thumb_checked"/>
            <corners android:radius="24dp"/>
            <size android:height="24dp" android:width="48dp"/>
        </shape>
    </item>
</selector>
